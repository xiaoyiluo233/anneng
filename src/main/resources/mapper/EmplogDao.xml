<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yidu.ly.dao.EmplogDao">

    <resultMap id="BaseResultMap" type="com.yidu.entity.Emplog">
        <!--@Table emplog-->
        <result property="elid" column="elid" jdbcType="INTEGER"/>
        <result property="eid" column="eid" jdbcType="INTEGER"/>
        <result property="type" column="type" jdbcType="INTEGER"/>
        <result property="message" column="message" jdbcType="VARCHAR"/>
        <result property="times" column="times" jdbcType="VARCHAR"/>
    </resultMap>

    <!--统计行数-->
    <select id="count" resultType="int">
        select
            count(0)
        from ane.emplog
        <where>
            <if test="name!=null and name!=''">
                name like '%${name}%'
            </if>
        </where>
    </select>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          elid, eid, type, message, times
        from ane.emplog
        where elid = #{elid}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          elid, eid, type, message, times
        from ane.emplog
        <where>
            <if test="type!=null">
                type like #{type}
            </if>
        </where>
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          elid, eid, type, message, times
        from ane.emplog
        <where>
            <if test="elid != null">
                and elid = #{elid}
            </if>
            <if test="eid != null">
                and eid = #{eid}
            </if>
            <if test="type != null">
                and type = #{type}
            </if>
            <if test="message != null and message != ''">
                and message = #{message}
            </if>
            <if test="times != null and times != ''">
                and times = #{times}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="elid" useGeneratedKeys="true">
        insert into ane.emplog(eid, type, message, times)
        values (#{eid}, #{type}, #{message}, #{times})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update ane.emplog
        <set>
            <if test="eid != null">
                eid = #{eid},
            </if>
            <if test="type != null">
                type = #{type},
            </if>
            <if test="message != null and message != ''">
                message = #{message},
            </if>
            <if test="times != null and times != ''">
                times = #{times},
            </if>
        </set>
        where elid = #{elid}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from ane.emplog where elid = #{elid}
    </delete>

</mapper>